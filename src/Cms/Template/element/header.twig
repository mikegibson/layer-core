{% list_theme 'block/nav_list' %}

<header id="cms-header">

	{% block cms_header %}

		<h1>
			{% block cms_heading %}<a class="home" href="{{ url('app') }}">{{ app.name }}</a>{% endblock %}
		</h1>

		<nav role="navigation">

			{% if is_granted('ROLE_ADMIN') %}

			{% block cms_header_nav %}

			{{ list( app['cms.navigation_list'] ) }}

			{% endblock cms_header_nav %}

			{% endif %}

		</nav>

	{% endblock cms_header %}

</header>