{% extends '@cms/layout/cms_repository.twig' %}

{% if pluralName is not defined %}
	{% set pluralName = repository.getEntityHumanName({plural: true}) %}
{% endif %}

{% if title is not defined %}
	{% set title = 'List ' ~ pluralName %}
{% endif %}

{% block content_inner %}

	{{ table( paginator ) }}

	{{ paginator_links( paginator ) }}

{% endblock %}